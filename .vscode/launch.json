    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "va-cli(getagent)",
            "program": "${workspaceRoot}/node_modules/ts-node/dist/bin.js",
            "args": [
                "src/va-cli",
                "-g",
                "-d",
                "agentData",
                "getintent",
                "FedTheAnimal"
            ],
            "env": {
                "DEBUG": "va-cli*"
            },
            "console": "integratedTerminal",
            "outputCapture": "std"
            // ,
            // "outFiles": [
            //     "${workspaceFolder}/**/*.js"
            // ]
        }
    ]
}